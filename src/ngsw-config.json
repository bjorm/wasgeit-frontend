{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html"
      ],
      "versionedFiles": [
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "api",
    "urls": ["/rest/agenda", "/rest/news"],
    "version": 1,
    "cacheConfig": {
      "maxSize": 10,
      "maxAge": "1d",
      "timeout": "20s",
      "strategy": "freshness"
    }
  }]
}
